<!DOCTYPE html>
<html>
<head>
  <title>Art1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%= render "common/navbar" %>
		<%= notice_message %>
		<div class="row">
			<div class="col-md-2">
				<ul  class="nav nav-pills nav-stacked">
					<li>
						<%= link_to "Product List", admin_products_path %>
					</li>
					<li>
						<%= link_to "User List", "#" %>
					</li>
					<li>
						<%= link_to "Order List", "#" %>
					</li>
				</ul>
			</div>

			<div class="col-md-10">
				<%= yield %>
			</div>
		</div>
	</div>
</body>
</html>
