<div class="row">
	<div class="col-md-12">
		<h2>My Orders</h2>
		<table class="table table-bordered">
			<thead>
				<tr>
					<th>商品</th>
					<th>生成時間</th>
				</tr>
			</thead>
			<tbody>
				<% @orders.each do |order| %>
					<tr>
						<td><%= link_to order.id, order_path(order.token) %></td>
						<td><%= order.created_at.to_s(:long) %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>